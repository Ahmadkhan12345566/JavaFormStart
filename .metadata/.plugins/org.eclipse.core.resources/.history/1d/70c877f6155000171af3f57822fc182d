
public class Variables {


	 public void pupAge() {
	      int age = 0; //local variables
	      age = age + 7;
	      System.out.println("Puppy age is : " + age);
	   }
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		 Variables test = new Variables();
	      test.pupAge();
		
		
	}

}
