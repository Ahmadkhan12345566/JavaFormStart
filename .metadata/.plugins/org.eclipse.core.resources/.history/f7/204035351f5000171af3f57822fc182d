
public class loops {

	
	
	
	public static void main(String[] args) {
		
		// TODO Auto-generated method stub 
		String  str1="i am at 1";
		//use of for loop
		for(int i=0;i<20;i++){
			System.out.println(str1);
		}
		//use of while loop
		int a=0;
		while(a<5){
			System.out.println("I am in while loop");
			a++;
		}
		
		
		//do while
		int b=0;
		do{
			System.out.println("I am in do while loop");
			b++;
		}while(b<5);
		

	
	
	
	
	
	}

}
